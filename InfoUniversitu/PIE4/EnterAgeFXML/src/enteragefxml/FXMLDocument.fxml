<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Text?>

<GridPane alignment="center" hgap="10" vgap="10" fx:controller="enteragefxml.FXMLDocumentController" xmlns:fx="http://javafx.com/fxml/1">
    
<padding>
    <Insets bottom="25" left="25" right="25" top="25" />
</padding>
    
    <Text fx:id="scenetitle" text="Calculation of age" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="0" />
 
    <Label fx:id="userName" text="Name:" GridPane.columnIndex="0" GridPane.rowIndex="1" />
 
    <TextField fx:id="nameTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
 
    <Label fx:id="userYear" text="Year of birth:" GridPane.columnIndex="0" GridPane.rowIndex="2" />
 
    <TextField fx:id="valueYear" GridPane.columnIndex="1" GridPane.rowIndex="2" />
    
    <HBox alignment="bottom_right" spacing="10" GridPane.columnIndex="1" GridPane.rowIndex="4">
        <Button onAction="#handleButtonAction" text="Ok" />
    </HBox>

    <Text fx:id="actiontarget" GridPane.columnIndex="1" GridPane.rowIndex="6" />
    
</GridPane>
